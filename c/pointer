#include <stdio.h>
int main()
{
  int x, y, t;
  printf("Enter two integers");
  scanf("%d%d", &x, &y);
  printf("Before Swapping\nFirst integer = %d\nSecond integer = %d\n", x, y);
  t = x;
  x = y;
  y = t;
  printf("After Swapping\nFirst integer = %d\nSecond integer = %d\n", x, y);
  return 0;
}

